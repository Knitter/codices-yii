{
    "name": "knitter/codices-yii",
    "type": "project",
    "description": "Book, ebook and audiobook management application",
    "keywords": [
        "books",
        "ebooks",
        "audiobooks",
        "personal library management"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Knitter/codices-yii/issues?state=open",
        "source": "https://github.com/Knitter/codices-yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "ext-dom": "*",
        "ext-tidy": "*",
        "ext-libxml": "*",
        "ext-gd": "*",
        "ext-pgsql": "*",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "vlucas/phpdotenv": "^5.6",
        "yiisoft/yii2": "^2.0.53",
        "yiisoft/yii2-swiftmailer": "~2.1.0"
    },
    "require-dev": {
        "yiisoft/yii2-apidoc": "^3.0",
        "yiisoft/yii2-debug": "^2.1",
        "yiisoft/yii2-faker": "^2.0",
        "cyclonedx/cyclonedx-php-composer": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Codices\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codices\\Tests\\": "tests"
        }
    },
    "extra": {
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "composer/package-versions-deprecated": true,
            "cyclonedx/cyclonedx-php-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
