{
    "name": "knitter/codices-yii",
    "type": "project",
    "description": "Book, ebook and audiobook management application",
    "keywords": [
        "yii3",
        "books",
        "ebooks",
        "audiobooks",
        "personal library management"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Knitter/codices-yii/issues?state=open",
        "source": "https://github.com/Knitter/codices-yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@php ./yii serve"
        ],
        "test": "codecept run",
        "test-watch": "phpunit-watcher watch"
    },
    "require": {
        "php": "^8.4",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "httpsoft/http-message": "^1.1.6",
        "psr/container": "^2.0.2",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0.2",
        "psr/http-server-middleware": "^1.0.2",
        "psr/log": "^3.0.2",
        "symfony/console": "^7.3.3",
        "vlucas/phpdotenv": "^5.6.2",
        "webmozart/assert": "^1.11",
        "yiisoft/config": "^1.6",
        "yiisoft/di": "^1.4",
        "yiisoft/error-handler": "^4.3",
        "yiisoft/log": "^2.1.1",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/middleware-dispatcher": "^5.3",
        "yiisoft/router": "^4.0.1",
        "yiisoft/router-fastroute": "^4.0.2",
        "yiisoft/yii-console": "^2.3",
        "yiisoft/arrays": "^3.2",
        "yiisoft/assets": "^5.1.1",
        "yiisoft/csrf": "^2.2.2",
        "yiisoft/aliases": "^3.1",
        "yiisoft/yii-view-renderer": "^7.3",
        "yiisoft/yii-http": "^1.1.0",
        "yiisoft/yii-runner-console": "^2.2",
        "yiisoft/yii-runner-http": "^3.2"
    },
    "require-dev": {
        "codeception/module-asserts": "^3.0",
        "codeception/module-cli": "^2.0",
        "codeception/module-phpbrowser": "^3.0",
        "maglnet/composer-require-checker": "^4.5",
        "rector/rector": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "Codices\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codices\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin-file": "configuration.php"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/config": true,
            "infection/extension-installer": true,
            "codeception/c3": true,
            "composer/installers": true
        }
    }
}
